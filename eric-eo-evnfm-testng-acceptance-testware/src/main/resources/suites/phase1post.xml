<suite name="Phase 2 TestNg Suite" verbose="10" configfailurepolicy="continue" preserve-order="true">

  <listeners>
    <listener class-name="com.ericsson.eo.evnfm.acceptance.testng.listeners.EvnfmTestNGListener"/>
  </listeners>

  <parameter name="instantiateData" value="phases/phase1postInstantiate.yaml"/>

  <test name="Terminate and instantiate same CNF" parallel="none">
    <classes>
      <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.evnfmreleaseflow.instantiate.InstantiateRestTest">
        <methods>
          <include name="terminateCNF"/>
          <include name="instantiateCNF"/>
        </methods>
      </class>
    </classes>
  </test>
</suite>