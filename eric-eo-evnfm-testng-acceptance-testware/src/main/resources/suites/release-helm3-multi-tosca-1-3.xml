<suite name="E-VNFM Release TestNg Suite" verbose="10" configfailurepolicy="continue" preserve-order="true">

    <listeners>
        <listener class-name="com.ericsson.eo.evnfm.acceptance.testng.listeners.EvnfmTestNGListener"/>
    </listeners>

    <parameter name="testTargetClusterData" value="cismCluster/testTargetCluster.yaml"/>
    <parameter name="basicOnboardingData" value="helm3-multi-tosca-1-3/onboarding.yaml"/>
    <parameter name="basicLCMData" value="helm3-multi-tosca-1-3/basicLCM/basicLCM.yaml"/>
    <parameter name="cnfHealData" value="helm3-multi-tosca-1-3/heal/healRest.yaml"/>
    <parameter name="adminScaleData" value="helm3-multi-tosca-1-3/adminScale/adminScale.yaml"/>
    <parameter name="backupUIData" value="helm3-multi-tosca-1-3/backup/backupUI.yaml"/>
    <parameter name="modifyVnfInfoRestData" value="helm3-multi-tosca-1-3/modifyVnfInfo/modifyVnfInfoRest.yaml"/>
    <parameter name="modifyVnfInfoUIData" value="helm3-multi-tosca-1-3/modifyVnfInfo/modifyVnfInfoUI.yaml"/>
    <parameter name="failOperationData" value="helm3-multi-tosca-1-3/failOperation/failOperation.yaml"/>
    <parameter name="rollbackAtSuccessData" value="helm3-multi-tosca-1-3/rollback/rollbackAtSuccess.yaml"/>
    <parameter name="rollbackAtFailureData" value="helm3-multi-tosca-1-3/rollback/rollbackAtFailure.yaml"/>
    <parameter name="syncData" value="helm3-multi-tosca-1-3/sync/sync.yaml"/>
    <parameter name="multipleScaleOperationData" value="helm3-multi-tosca-1-3/scale/multipleScaleOperationTest.yaml"/>
    <parameter name="persistenceOfScalingData" value="helm3-multi-tosca-1-3/scale/persistScaleAtUpgrade.yaml"/>
    <parameter name="persistenceOfScalingUIData" value="helm3-multi-tosca-1-3/scale/persistenceOfScalingUI.yaml"/>
    <parameter name="cnfScaleData" value="helm3-multi-tosca-1-3/scale/scaleCnf.yaml"/>

    <test name="Set up" parallel="none">
        <classes>
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.cismcluster.BasicClusterTest">
                <methods>
                    <include name="testRegisterCluster"/>
                </methods>
            </class>
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.onboarding.BasicOnboardingTest">
                <methods>
                    <include name="testLicensesAvailable"/>
                    <include name="testOnboardCsarREST"/>
                    <include name="testVnfdZipOnboardedCorrectly"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="End to End Acceptance Tests" parallel="classes" thread-count="THREAD_COUNT">
        <classes>
<!--        <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.ui.lcm.scale.MultipleScaleOperationTest"/>&lt;!&ndash;21 minutes&ndash;&gt;-->
<!--        <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.ui.lcm.scale.AdminScaleUITest"/>&lt;!&ndash;15 minutes&ndash;&gt;-->
<!--        <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.ui.lcm.scale.PersistenceOfScalingDataUITest"/>&lt;!&ndash;15 minutes&ndash;&gt;-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.rollback.RollbackAtSuccessTest"/><!--15 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.rollback.RollbackAtFailureTest"/><!--11 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.sync.SyncTest"/><!--10 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.BasicLCMTest"/><!--7 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.backuprestore.BackupRestoreOperationRestTest"/><!--7 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.scale.PersistenceOfScalingDataTest"/><!--7 minutes-->
<!--            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.ui.lcm.backup.BackupUITest"/> &lt;!&ndash;6 minutes&ndash;&gt;-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.rollback.FailOperationTest"/><!--5 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.modify.ModifyVnfInfoSuccessTest"/><!--5 minutes-->
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.lcm.scale.ScaleSuccessTest"/><!--4 minutes-->
        </classes>
    </test>
    <test name="Clean up" parallel="none">
        <classes>
            <class name="com.ericsson.eo.evnfm.acceptance.testng.tests.rest.onboarding.BasicOnboardingTest">
                <methods>
                    <include name="testDeletePackageREST"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>
